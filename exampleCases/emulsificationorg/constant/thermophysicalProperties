FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water oil air);

pMin            -1e11; //-1e11;
pMinGas         0.1; //-1e11;

sigmas
(
    (air water)     0.07
    (air oil)       0.02
    (water oil)     0.03
);


